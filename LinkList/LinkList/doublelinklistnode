
namespace LinkList
{
    class LInkedListNode<T>
    {
        public LInkedListNode(T value)
        {
            Value = value;
        }

        public T Value { get; set; }

        public LInkedListNode<T> Next { get; set; }
        public LInkedListNode<T> Previous { get; set; } //for double link list
    }
}
